#include <stdio.h>
#include <locale.h>

 main() {

  setlocale(LC_ALL, "portuguese");

   //utilizei o chatGPT para entender melhor como fazer isso...

   int N;

   printf("Coloque o valor de N: ");
   scanf("%d", &N);

   N = (N < 0) ? 0 : N;

   int impar = 1;

   for(int i = 0; i < N; i++) {

    printf("%d\n", impar);

     impar += 2;
     
   }

   
     
}
