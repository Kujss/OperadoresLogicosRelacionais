#include<stdio.h>
#include<math.h>
#include<locale.h>

main (){
	
	setlocale(LC_ALL, "portuguese");
	
	double num, raiz, quad;
	
	printf("Digite o número: ");
	scanf("%lf", &num);
	
	if(num >= 0){
		
		raiz = sqrt(num);
		printf("A raiz quadrada é: %lf", raiz);
		
	}else{
		
		quad = pow(num, 2);
		printf("O quadrado é: %lf", quad);
		
	}
	
}
